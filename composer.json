{
    "name": "prune-mazui/zephir-ide-helper",
    "description": "Make zephir code (.zep) to PHP code completion File.",
    "keywords": ["zephir", "IDE", "intellisense"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/PruneMazui/zephir-ide-helper",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "PruneMazui\\ZephirIdeHelper\\" : "src"
        }
    },
    "scripts": {
        "pre-update-cmd": [
            "php -r \"if (!function_exists('zephir_parse_file')) {throw new \\Exception();}\""
        ]
    }
}
