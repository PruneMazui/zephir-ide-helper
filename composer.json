{
    "name": "prune-mazui/zephir-ide-helper",
    "description": "Generate php code completion file from zephir file(.zep).",
    "keywords": ["zephir", "ide", "php", "code-generator"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/PruneMazui/zephir-ide-helper",
    "require": {
        "php": ">=7.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PruneMazui\\ZephirIdeHelper\\" : "src"
        }
    },
    "bin": [
        "bin/zephir-ide-helper"
    ]
}
